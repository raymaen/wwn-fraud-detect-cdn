async function postToMultipleUrls(t,r){let a=t.map(async t=>{try{let a=await fetch(t,{method:"POST",headers:{"Content-Type":"application/json"},body:JSON.stringify(r)});if(!a.ok)throw Error(`Request to ${t} failed with status ${a.status}`);return a.json()}catch(o){throw console.error(`Error posting to ${t}:`,o),o}});try{let o=await Promise.all(a);return o}catch(e){console.error("Error posting to one or more URLs:",e)}}const urls=["https://catfact.ninja/fact","https://api.agify.io/?name=meelad","https://api.agify.io/?name=john","https://api.agify.io/?name=babu",],data={};postToMultipleUrls(urls,data).then(t=>{console.log("All requests successful:",t)}).catch(t=>{console.error("Error occurred:",t)});
